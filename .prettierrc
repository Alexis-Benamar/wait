{
  "printWidth": 120,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^(components|pages)?/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}